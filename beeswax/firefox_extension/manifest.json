{
  "manifest_version": 2,
  "name": "BeesWax",
  "version": "1.0.2",
  "permissions": [
      "tabs",
      "activeTab",
      "https://beeswax.onrender.com/*"
  ],
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  },
  "applications": {
      "gecko": {
          "id": "beeswax@dev.version",
          "strict_min_version": "91.0"
      }
  }
}

  